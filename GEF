local Library = loadstring(Game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wizard"))()
local Window = Library:NewWindow("• WinterClaws •")
local Farm = Window:NewSection("Cày")
Farm:CreateTextbox("Thời Gian Cày",function(text)
getgenv().CooldownFarm = v
end)
Farm:CreateToggle("Cày Tiền", function(value)
getgenv().FarmMoney = v
while getgenv().FarmMoney do wait(getgenv().CooldownFarm or 0)
for _, v in  pairs(workspace:GetDescendants()) do
if v.Name == "Money" then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Pickups.Money.CFrame
wait(0.2)
fireproximityprompt(v.ProximityPrompt)
end
end
end
end)
